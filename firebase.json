{
  "functions": {
    "predeploy": [
      "nx build backend-functions --skip-nx-cache --prod"
    ],
    "source": "dist/apps/backend-functions"
  },
  "hosting": {
    "public": "dist/apps/dashboard",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
