{
  "npmScope": "parrotly.io",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "background": {
      "tags": []
    },
    "background-e2e": {
      "tags": [],
      "implicitDependencies": ["background"]
    },
    "constants": {
      "tags": []
    },
    "content-script": {
      "tags": []
    },
    "content-script-e2e": {
      "tags": [],
      "implicitDependencies": ["content-script"]
    },
    "dashboard": {
      "tags": []
    },
    "dashboard-e2e": {
      "tags": [],
      "implicitDependencies": ["dashboard"]
    },
    "popup": {
      "tags": []
    },
    "popup-e2e": {
      "tags": [],
      "implicitDependencies": ["popup"]
    }
  }
}
